{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": true
        },
        "target": "es2022",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": false,
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "module": {
        "type": "commonjs"
    },
    "minify": false,
    "isModule": true
}
